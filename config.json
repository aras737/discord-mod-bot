// config.json içeriğini güncelleyen Node.js kodu
const fs = require('fs');

const configPath = './config.json';
const config = JSON.parse(fs.readFileSync(configPath, 'utf-8'));

delete config.roleMap["2"];
delete config.roleMap["3"];

fs.writeFileSync(configPath, JSON.stringify(config, null, 2), 'utf-8');

console.log('✅ roleMap içinden 2 ve 3 silindi.');
